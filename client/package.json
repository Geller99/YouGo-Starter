{
  "name": "client",
  "version": "1.0.0",
  "description": "client-side",
  "main": "App.js",
  "watch": {
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "test": "jest",
    "watch": "npm-watch",
    "start": "run-script-os",
    "start:win32": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Google\\Chrome\\Application\\chrome\" ",
    "start-fox": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
    "//": "Hello! If you are having trouble running this command. Try changing browsers on here",
    "start:darwin:linux": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser 'Firefox Developer Edition'"
  },
  "author": "Geller Micael",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.2.3"
  },
  "dependencies": {
    "browser-sync": "^2.27.5",
    "run-script-os": "^1.1.6"
  }
}
